{
  "name": "@kubernetes-models/cert-manager",
  "version": "4.4.4",
  "description": "cert-manager models",
  "repository": {
    "type": "git",
    "url": "https://github.com/tommy351/kubernetes-models-ts.git"
  },
  "homepage": "https://github.com/tommy351/kubernetes-models-ts/tree/master/third-party/cert-manager",
  "author": "Tommy Chen <tommy351@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "crd-generate && publish-scripts build",
    "prepack": "publish-scripts prepack"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist",
    "linkDirectory": true
  },
  "keywords": [
    "kubernetes",
    "kubernetes-models",
    "cert-manager"
  ],
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@kubernetes-models/apimachinery": "workspace:^",
    "@kubernetes-models/base": "workspace:^",
    "@kubernetes-models/validate": "workspace:^",
    "@swc/helpers": "^0.5.8"
  },
  "devDependencies": {
    "@kubernetes-models/crd-generate": "workspace:^",
    "@kubernetes-models/publish-scripts": "workspace:^",
    "vitest": "^0.29.8"
  },
  "crd-generate": {
    "input": [
      "https://raw.githubusercontent.com/cert-manager/cert-manager/v0.10.1/deploy/manifests/00-crds.yaml",
      "https://raw.githubusercontent.com/cert-manager/cert-manager/v0.14.3/deploy/manifests/00-crds.yaml",
      "https://github.com/cert-manager/cert-manager/releases/download/v0.16.1/cert-manager.crds.yaml",
      "https://github.com/cert-manager/cert-manager/releases/download/v1.5.5/cert-manager.crds.yaml",
      "https://github.com/cert-manager/cert-manager/releases/download/v1.17.2/cert-manager.crds.yaml",
      "https://raw.githubusercontent.com/cert-manager/trust-manager/v0.4.0/deploy/crds/trust.cert-manager.io_bundles.yaml",
      "https://raw.githubusercontent.com/cert-manager/approver-policy/v0.6.3/deploy/charts/approver-policy/templates/crds/policy.cert-manager.io_certificaterequestpolicies.yaml",
      "https://raw.githubusercontent.com/cert-manager/aws-privateca-issuer/v1.2.2/charts/aws-pca-issuer/crds/awspca.cert-manager.io_awspcaclusterissuers.yaml",
      "https://raw.githubusercontent.com/cert-manager/aws-privateca-issuer/v1.2.2/charts/aws-pca-issuer/crds/awspca.cert-manager.io_awspcaissuers.yaml"
    ],
    "output": "./gen"
  }
}

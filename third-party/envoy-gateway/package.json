  },
  "devDependencies": {
    "@kubernetes-models/crd-generate": "workspace:^",
    "@kubernetes-models/publish-scripts": "workspace:^",
    "@kubernetes-models/read-input": "workspace:^",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.7.23",
    "js-yaml": "^4.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "vitest": "^0.29.8"
  },
  "crd-generate": {
    "input": [

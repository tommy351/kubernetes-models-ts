{
  "name": "@kubernetes-models/victoria-metrics-operator",
  "version": "2.6.3",
  "description": "VictoriaMetrics operator models",
  "repository": {
    "type": "git",
    "url": "https://github.com/tommy351/kubernetes-models-ts.git"
  },
  "homepage": "https://github.com/tommy351/kubernetes-models-ts/tree/master/third-party/victoria-metrics-operator",
  "author": "Tommy Chen <tommy351@gmail.com",
  "license": "MIT",
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "crd-generate && publish-scripts build",
    "prepack": "publish-scripts prepack"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist",
    "linkDirectory": true
  },
  "keywords": [
    "kubernetes",
    "kubernetes-models",
    "victoria-metrics-operator"
  ],
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@kubernetes-models/apimachinery": "workspace:^",
    "@kubernetes-models/base": "workspace:^",
    "@kubernetes-models/validate": "workspace:^",
    "@swc/helpers": "^0.5.8"
  },
  "devDependencies": {
    "@kubernetes-models/crd-generate": "workspace:^",
    "@kubernetes-models/publish-scripts": "workspace:^",
    "@types/find-cache-dir": "^3.2.1",
    "@types/make-fetch-happen": "^10.0.0",
    "@types/node": "^18.7.23",
    "@types/stream-to-promise": "^2.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "vitest": "^0.29.8"
  },
  "crd-generate": {
    "input": [
      "https://github.com/VictoriaMetrics/operator/releases/download/v0.49.1/crd.yaml"
    ],
    "output": "./gen"
  }

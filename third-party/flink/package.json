{
  "name": "@kubernetes-models/flink",
  "version": "0.0.0",
  "description": "Flink models",
  "repository": {
    "type": "git",
    "url": "https://github.com/tommy351/kubernetes-models-ts.git"
  },
  "homepage": "https://github.com/tommy351/kubernetes-models-ts/tree/master/third-party/flink",
  "author": "Tommy Chen <tommy351@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "crd-generate && publish-scripts build",
    "prepack": "publish-scripts prepack"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist",
    "linkDirectory": true
  },
  "keywords": [
    "kubernetes",
    "kubernetes-models",
    "flink"
  ],
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@kubernetes-models/apimachinery": "workspace:^",
    "@kubernetes-models/base": "workspace:^",
    "@kubernetes-models/validate": "workspace:^",
    "@swc/helpers": "^0.5.8"
  },
  "devDependencies": {
    "@kubernetes-models/crd-generate": "workspace:^",
    "@kubernetes-models/publish-scripts": "workspace:^",
    "vitest": "^0.29.8"
  },
  "crd-generate": {
    "input": [
      "https://raw.githubusercontent.com/apache/flink-kubernetes-operator/refs/tags/release-1.12.1/helm/flink-kubernetes-operator/crds/flinkdeployments.flink.apache.org-v1.yml",
      "https://raw.githubusercontent.com/apache/flink-kubernetes-operator/refs/tags/release-1.12.1/helm/flink-kubernetes-operator/crds/flinksessionjobs.flink.apache.org-v1.yml",
      "https://raw.githubusercontent.com/apache/flink-kubernetes-operator/refs/tags/release-1.12.1/helm/flink-kubernetes-operator/crds/flinkstatesnapshots.flink.apache.org-v1.yml"
    ],
    "output": "./gen"
  }
}

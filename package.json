{
  "name": "kubernetes-models",
  "version": "0.0.0",
  "private": true,
  "description": "Kubernetes models",
  "repository": {
    "type": "git",
    "url": "https://github.com/tommy351/kubernetes-models-ts.git"
  },
  "homepage": "https://github.com/tommy351/kubernetes-models-ts",
  "author": "Tommy Chen <tommy351@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "keywords": [
    "kubernetes"
  ],
  "scripts": {
    "lint": "eslint . --ext .js,.ts,.cjs,.mjs",
    "build": "tsc-multi packages/openapi-generate packages/crd-generate packages/export-map && lerna run build --stream && tsc-multi",
    "watch": "npm run build -- --watch",
    "clean": "rm -rf packages/*/dist && tsc-multi --clean && lerna run clean",
    "bootstrap": "lerna bootstrap",
    "prepack": "lerna run prepack",
    "test": "npm-run-all test:*",
    "test:unit": "jest",
    "test:esm": "ava integration/esm/__tests__/**/*",
    "new-crd-package": "ts-node scripts/new-crd-package.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,cjs,mjs}": "eslint --fix"
  },
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.8",
    "@types/signale": "^1.4.1",
    "@types/yargs": "^15.0.10",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "ava": "^3.15.0",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.5.3",
    "lerna": "^3.22.1",
    "lint-staged": "^10.4.0",
    "make-dir": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "signale": "^1.4.0",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "tsc-multi": "^0.3.1",
    "typescript": "^4.0.3",
    "yargs": "^16.1.1"
  }
}

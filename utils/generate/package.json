{
  "name": "@kubernetes-models/generate",
  "version": "2.3.0",
  "description": "Type declarations and utilities for Kubernetes model generation.",
  "repository": {
    "type": "git",
    "url": "https://github.com/tommy351/kubernetes-models-ts.git"
  },
  "homepage": "https://github.com/tommy351/kubernetes-models-ts/tree/master/utils/generate",
  "author": "Tommy Chen <tommy351@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc-multi --config ../../tsc-multi.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "keywords": [
    "kubernetes",
    "kubernetes-models"
  ],
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/bin/",
    "/dist/"
  ],
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@kubernetes-models/string-util": "workspace:^",
    "ajv": "^8.12.0",
    "fs-extra": "^10.1.0",
    "indent-string": "^4.0.0",
    "lodash": "^4.17.21",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.186",
    "tsc-multi": "^0.6.1",
    "typescript": "^4.8.4"
  }
}
